    script.
      $(function () {
        var $mlNav = $('.ml-nav');
        $('#dowebok').fullpage({
          //sectionsColor: ['#238C00', '#FA895D', '#ED6374', '#f90'],
          navigationTooltips: ['首页', '更新日志', '加入我们', '关于有车约'],
          navigation: true,
          anchors: ['page1', 'page2', 'page3', 'page4'],
          menu: '#menu',
          scrollingSpeed: 1000,
          scrollOverflow: true,
          afterRender: function () {
            $('.section1 h2').fadeIn(1500);
            $('.section1 .btn').animate({bottom: '80px'}, 500);
          },
          afterLoad: function (anchorLink, index) {
            if (index == 1) {
              $('.section1 h2').fadeIn(1800);
              $('.section1 .btn').animate({bottom: '80px'}, 500);
              //$(this).find('h2').animate({top: '25%'},500);
              $(this).find('.apple').fadeIn();
              $(this).find('.android').delay(500).fadeIn("slow");
              $(this).find('.scan-QR-code').delay(900).fadeIn("slow");
              //$(this).find('.scan-QR-code').delay(1500).fadeIn("slow");
            }
            if (index == 2) {
              $(".section2").each(function () {
                $(this).find('h2').animate({top: '25%'}, 500);
                $(this).find('.text').delay(500).animate({left: '0'}, 500);
                $(this).find('.imgbg').delay(900).animate({right: '0'}, 500);
                $(this).find('.bg2').fadeIn(500);
              });
            }
            if (index == 3) {
              $(".section3").each(function () {
                $(this).find('h2').animate({top: '200px'}, 500);
                $(this).find('h3').animate({top: '270px'}, 500);
                $(this).find('.text').animate({left: '0'}, 500);
                $(this).find('.imgbg').animate({right: '0'}, 500);
                $(this).find('.bg3').delay(700).slideDown(500);
              });
            }
            if (index == 4) {
              $(".section4").each(function () {
                $(this).find('h2').animate({top: '25%'}, 500);
                $(this).find('.text').animate({left: '0'}, 500);
                $(this).find('.imgbg').animate({right: '0'}, 500);
                $(this).find('.bg4').fadeIn(500);
              });
            }
            if (index == 5) {
              $(".section5").each(function () {
                $(this).find('h2').animate({top: '25%'}, 500);
                $(this).find('.text').animate({left: '0'}, 500);
                $(this).find('.imgbg').animate({right: '0'}, 500);
                $(this).find('.bg5').fadeIn(500);
              });
            }
            if (index == 6) {
              $(".section6").each(function () {
                $('.section6 .btn').animate({bottom: '80px'}, 500);
                $(this).find('h2').animate({top: '25%'}, 500);
                $(this).find('.t1').fadeIn();
                $(this).find('.t2').delay(500).fadeIn("slow");
                $(this).find('.t3').delay(900).fadeIn("slow");
              });
            }
          },
          onLeave: function (index, nextIndex, direction) {
            if (index == 2 && direction == 'up') {
              $mlNav.animate({top: '5%'}, 680);
            } else if (index == 1 && direction == 'down') {
              $mlNav.animate({top: '-90'}, 400);
            } else if (index == 3 && direction == 'up') {
              $mlNav.animate({top: '-90'}, 500);
            } else {
              $mlNav.animate({top: '-90'}, 400);
            }
            if (index == '1') {
              $('.section1 h2').fadeOut(1000);
              $('.section1 .btn').animate({bottom: '-120%'}, 500);
              $(this).find('.apple').delay(900).fadeOut("slow");
              $(this).find('.android').delay(500).fadeOut("slow");
              $(this).find('.windows').delay(200).fadeOut("slow");
              $(this).find('.scan-QR-code').fadeOut();
            }
            if (index == '2') {
              $(".section2").each(function () {
                $(this).find('h2').animate({top: '-120%'}, 1500);
                $(this).find('.text').animate({left: '-120%'}, 1500);
                $(this).find('.imgbg').animate({right: '-120%'}, 1500);
                $(this).find('.bg2').fadeOut(500);
              });
            }
            if (index == '3') {
              $(".section3").each(function () {
                $(this).find('h2').animate({top: '-120%'}, 1500);
                $(this).find('h3').animate({top: '-120%'}, 1500);
                $(this).find('.text').animate({left: '-120%'}, 1500);
                $(this).find('.imgbg').animate({right: '-120%'}, 1500);
                $(this).find('.bg3').delay(700).slideUp(500);
              });
            }
            if (index == '4') {
              $(".section4").each(function () {
                $(this).find('h2').animate({top: '-120%'}, 1500);
                $(this).find('.text').animate({left: '-120%'}, 1500);
                $(this).find('.imgbg').animate({right: '-120%'}, 1500);
                $(this).find('.bg4').fadeOut(500);
              });
            }
            if (index == '5') {
              $(".section5").each(function () {
                $(this).find('h2').animate({top: '-120%'}, 1500);
                $(this).find('.text').animate({left: '-120%'}, 1500);
                $(this).find('.imgbg').animate({right: '-120%'}, 1500);
                $(this).find('.bg5').fadeOut(500);
              });
            }
            if (index == '6') {
              $('.section6 .btn').animate({bottom: '-120%'}, 500);
              $(".section6").each(function () {
                $(this).find('h2').animate({top: '-125%'}, 500);
                $(this).find('.t1').delay(900).fadeOut("slow");
                $(this).find('.t2').delay(500).fadeOut("slow");
                $(this).find('.t3').fadeOut();
              });
            }
          }
        });
      });
